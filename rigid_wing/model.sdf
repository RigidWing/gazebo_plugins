    <joint name='propeller_joint' type='revolute'>
     <parent>main_wing</parent>
     <child>propeller</child>
     <pose frame=''>0.1 0.0 1.475 0 1.57 0</pose>
     <axis>
       <xyz>0 0 1</xyz>
       <limit>
         <lower>-1e+12</lower>
         <upper>1e+12</upper>
         <effort>-1</effort>
         <velocity>-1</velocity>
       </limit>
       <dynamics>
         <damping>0.001</damping>
       </dynamics>
     </axis>
     <physics>
       <ode>
         <implicit_spring_damper>1</implicit_spring_damper>
       </ode>
     </physics>
    </joint>

    <joint name='elevator_joint' type='revolute'>
      <parent>main_wing</parent>
      <child>elevator</child>
      <pose frame=''>-1.44715 0 1.475 1.57 3.14159265359 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <limit>
          <lower>-6.981</lower>
          <upper>6.981</upper>
          <effort>-1</effort>
          <velocity>-1</velocity>
        </limit>
        <dynamics>
          <damping>1.000</damping>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <joint name='rudder_joint' type='revolute'>
      <parent>main_wing</parent>
      <child>rudder</child>
      <pose frame=''>-1.44715 0 0 1.57 3.14159265359 0</pose> <!--pose frame=''>-0.843084 0 1.475 1.57 3.14159265359 0</pose-->
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <limit>
          <lower>-0.5</lower>
          <upper>0.5</upper>
          <effort>-1</effort>
          <velocity>-1</velocity>
        </limit>
        <dynamics>
          <damping>1.000</damping>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <joint name='fixed_to_main_wing_left_wing' type='fixed'>
      <parent>main_wing</parent>
      <child>vertical_wing_left</child>
      <pose>0 0 0 0 0 0</pose>
        <sensor type="force_torque" name="force_torque_vertical_wing_left">
          <always_on>true</always_on>
          <topic>/force_torque_data_vertical_wing_left</topic>
          <visualize>true</visualize>
          <update_rate>100</update_rate>
          <force_torque>
            <frame>child</frame>
            <measure_direction>child_to_parent</measure_direction>
          </force_torque>
        </sensor>
    </joint>

    <joint name='fixed_to_main_wing_right_wing' type='fixed'>
