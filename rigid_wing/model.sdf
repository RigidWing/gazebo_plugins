    <plugin name='gazebo_control' filename='libgazebo_control_plugin.so'>
    <elevator_joint_name>elevator_joint</elevator_joint_name>
    <rudder_joint_name>rudder_joint</rudder_joint_name>
    <link_name>elevator</link_name>
    <elevator_p_gain>1</elevator_p_gain>
    <elevator_i_gain>0.01</elevator_i_gain>
    <elevator_d_gain>0</elevator_d_gain>
    <rudder_p_gain>1</rudder_p_gain>
    <rudder_i_gain>0.01</rudder_i_gain>
    <rudder_d_gain>1</rudder_d_gain>
  </plugin>

