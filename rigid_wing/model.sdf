    <joint name='elevator_joint' type='revolute'>
      <parent>main_wing</parent>
      <child>elevator</child>
      <pose frame=''>-0.843084 0 1.475 1.57 3.14159265359 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <limit>
          <lower>-6.981</lower>
          <upper>6.981</upper>
          <effort>-1</effort>
          <velocity>-1</velocity>
        </limit>
        <dynamics>
          <damping>1.000</damping>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <joint name='rudder_joint' type='revolute'>
      <parent>main_wing</parent>
      <child>rudder</child>
      <pose frame=''>-0.843084 0 1.475 1.57 3.14159265359 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <limit>
          <lower>-6.981</lower>
          <upper>6.981</upper>
          <effort>-1</effort>
          <velocity>-1</velocity>
        </limit>
        <dynamics>
          <damping>1.000</damping>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

